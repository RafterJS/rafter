{
    "name": "rafter",
    "version": "0.1.0",
    "contributors": [
        {
            "name": "Josh Stuart",
            "email": "joshstuartx@gmail.com"
        }
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/crimsonronin/rafter.git"
    },
    "scripts": {
        "dev": "pm2 --watch ./lib --watch ./config --watch ./example --watch ./rafter.js ./example/rafter-example.js --exec babel-node",
        "test": "TODO",
        "build-lib": "babel ./index.js --out-dir ./dist && babel ./rafter.js --out-dir ./dist && babel --include-dotfiles ./lib --out-dir ./dist/lib",
        "clean": "rimraf ./dist",
        "build": "yarn clean && yarn build-lib && yarn copy",
        "copy": "copyfiles ./package.json ./dist/package.json",
        "lint": "./node_modules/.bin/eslint ./.bin ./example ./lib ./index.js ./rafter.js",
        "prettier": "./node_modules/.bin/prettier --write \"{lib,example}/**/*.{js,jsx,ts,tsx}\""
    },
    "dependencies": {
        "box-di-autoloader": "^0.3.0",
        "express": "4.16.2"
    },
    "description": "A slightly opinionated JS framework that makes building apps easy and testable",
    "main": "./index.js",
    "devDependencies": {
        "@babel/cli": "^7.4.3",
        "@babel/core": "^7.4.3",
        "@babel/node": "^7.2.2",
        "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
        "@babel/preset-env": "^7.4.3",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^24.7.1",
        "copyfiles": "^2.1.0",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^4.1.0",
        "eslint-plugin-import": "^2.17.2",
        "jest": "^24.7.1",
        "pm2": "^3.5.0",
        "prettier": "^1.17.0",
        "rimraf": "^2.6.3"
    },
    "engines": {
        "node": ">9"
    }
}
