{
  "name": "@rafter/di-autoloader-example",
  "version": "0.0.1",
  "author": "Josh Stuart <joshstuartx@gmail.com>",
  "description": "An autoloading example",
  "keywords": [
    "di",
    "factory",
    "dependency injection",
    "ioc",
    "inversion of control",
    "solid"
  ],
  "main": "dist/example.js",
  "scripts": {
    "babel-node": "babel-node",
    "start": "pm2 start ./example/example.js --watch",
    "build:example": "babel ./index.js --out-dir ./dist && babel lib --out-dir ./dist/lib && babel ./example --out-dir ./dist/example",
    "build": "yarn clean && yarn build:example",
    "clean": "rimraf ./dist",
    "lint": "./node_modules/.bin/eslint lib",
    "prettier": "./node_modules/.bin/prettier --write \"{lib}/**/*.{js,jsx,ts,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crimsonronin/rafter.git"
  },
  "license": "MIT",
  "dependencies": {
    "@rafter/di-container": "0.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.17.2",
    "pm2": "^3.5.0",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3"
  },
  "engines": {
    "node": ">9"
  }
}
