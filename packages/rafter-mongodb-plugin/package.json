{
  "name": "@rafterjs/mongodb-plugin",
  "version": "0.4.2-alpha.2",
  "contributors": [
    {
      "name": "Josh Stuart",
      "email": "joshstuartx@gmail.com"
    }
  ],
  "license": "GNU GPLv3",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshystuart/rafter.git"
  },
  "scripts": {
    "build": "yarn clean && yarn build:lib && yarn build:config && yarn build:types",
    "build:lib": "babel lib --out-dir dist/lib --extensions .js,.jsx,.ts,.tsx --source-maps",
    "build:config": "babel config --out-dir dist/config --extensions .js,.jsx,.ts,.tsx --source-maps",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf ./dist",
    "lint": "eslint \"./lib/**/*\" ./babel.config.js",
    "format": "prettier --write \"lib/**/*.{js,jsx,ts,tsx}\"",
    "start:dev": "nodemon -e ts --watch \"./lib/**/*.ts\" --exec \"yarn build\""
  },
  "dependencies": {
    "@rafterjs/logger-plugin": "^0.4.2-alpha.0",
    "mongodb": "3.5.9"
  },
  "devDependencies": {
    "@rafterjs/babel-preset-rafter": "^0.4.2-alpha.0",
    "@rafterjs/eslint-config": "^0.4.2-alpha.0",
    "@types/mongodb": "^3.5.17"
  },
  "description": "A simple mongodb plugin",
  "main": "./dist/lib/mongoDb.js",
  "module": "./dist/lib/mongoDb.js",
  "typings": "./dist/lib/mongoDb.d.ts",
  "engines": {
    "node": ">=9"
  },
  "gitHead": "cec73cfa8cbfe5bd8422f1ea53f1711330514cc3"
}
