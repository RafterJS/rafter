{
  "name": "@rafter/babel-preset-rafter",
  "version": "0.0.1",
  "contributors": [
    {
      "name": "Josh Stuart",
      "email": "joshstuartx@gmail.com"
    }
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn clean:lib && yarn build:index",
    "build:index": "babel index.js --out-dir dist --extensions .js,.jsx,.ts,.tsx --source-maps",
    "clean:lib": "rimraf ./dist",
    "lint": "eslint ./index.js ./babel.config.js",
    "update:latest": "yarn upgrade --latest"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/crimsonronin/rafter.git"
  },
  "dependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/helper-plugin-utils": "7.8.3",
    "@babel/node": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.9.0",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-typescript": "7.9.0",
    "@babel/register": "7.9.0",
    "@babel/runtime-corejs2": "7.9.2",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.2.4",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.2"
  },
  "devDependencies": {
    "@rafter/eslint-config": "*"
  }
}
