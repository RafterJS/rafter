{
  "name": "@rafterjs/babel-preset-rafter",
  "version": "0.6.0",
  "contributors": [
    {
      "name": "Josh Stuart",
      "email": "joshstuartx@gmail.com"
    }
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "build": "yarn clean:lib && yarn build:index",
    "build:index": "babel index.js --out-dir dist --extensions .js,.jsx,.ts,.tsx --source-maps",
    "clean:lib": "rimraf ./dist",
    "lint": "eslint ./index.js ./babel.config.js",
    "update:latest": "yarn upgrade --latest"
  },
  "license": "GPL-3.0-or-later",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshystuart/rafter.git"
  },
  "dependencies": {
    "@babel/cli": "7.13.0",
    "@babel/core": "7.13.8",
    "@babel/helper-plugin-utils": "7.13.0",
    "@babel/node": "7.13.0",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "7.13.8",
    "@babel/plugin-proposal-optional-chaining": "7.13.8",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.13.9",
    "@babel/preset-typescript": "7.13.0",
    "@babel/register": "7.13.8",
    "@babel/runtime-corejs2": "7.13.9",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3"
  },
  "devDependencies": {
    "@rafterjs/eslint-config": "^0.6.0"
  },
  "gitHead": "a1f017d6435d2fd58ab8bfcaee868f5d63956807"
}
