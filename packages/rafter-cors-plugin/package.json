{
  "name": "@rafterjs/cors-plugin",
  "version": "0.6.0",
  "contributors": [
    {
      "name": "Josh Stuart",
      "email": "joshstuartx@gmail.com"
    }
  ],
  "license": "GPL-3.0-or-later",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshystuart/rafter.git"
  },
  "scripts": {
    "build": "yarn clean && yarn build:lib && yarn build:types",
    "start:watch": "nodemon --watch \"./lib\" --exec \"yarn build\"",
    "build:lib": "babel ./lib --out-dir ./dist --extensions .js,.jsx,.ts,.tsx --source-maps",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf ./dist",
    "format": "prettier --write \"lib/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"./lib/**/*\" ./babel.config.js",
    "start:dev": "nodemon -e ts --watch \"./**/*.ts\" --exec \"yarn build\"",
    "update:latest": "yarn upgrade --latest"
  },
  "dependencies": {
    "@rafterjs/logger-plugin": "^0.6.0",
    "cors": "2.8.5"
  },
  "devDependencies": {
    "@rafterjs/babel-preset-rafter": "0.6.0",
    "@rafterjs/eslint-config": "0.6.0",
    "@types/cors": "2.8.10"
  },
  "description": "A simple CORS plugin",
  "main": "./dist/cors.js",
  "module": "./dist/cors.js",
  "typings": "./dist/cors.d.ts",
  "engines": {
    "node": ">=9"
  },
  "gitHead": "a1f017d6435d2fd58ab8bfcaee868f5d63956807"
}
