{
  "name": "@rafterjs/cors-rafter-plugin",
  "version": "0.4.0",
  "contributors": [
    {
      "name": "Josh Stuart",
      "email": "joshstuartx@gmail.com"
    }
  ],
  "license": "GNU GPLv3",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshystuart/rafter.git"
  },
  "scripts": {
    "build": "yarn build:lib && yarn build:config && yarn build:types",
    "build:lib": "babel cors.ts --out-dir dist --extensions .js,.jsx,.ts,.tsx --source-maps",
    "build:config": "babel --include-dotfiles config --out-dir dist/config --extensions .js,.jsx,.ts,.tsx --source-maps",
    "build:types": "tsc --emitDeclarationOnly",
    "clean:lib": "rimraf ./dist",
    "lint": "eslint \"./cors.ts\" \"./config/**/*\"",
    "start:dev": "nodemon -e ts --watch \"./cors.ts\" --watch \"./config/**/*.ts\" --exec \"yarn build\""
  },
  "dependencies": {
    "@rafterjs/utils": "^0.4.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@rafterjs/babel-preset-rafter": "^0.4.0",
    "@rafterjs/eslint-config": "^0.4.0",
    "@types/cors": "^2.8.6"
  },
  "description": "A simple CORS plugin",
  "main": "./dist/cors.js",
  "module": "./dist/cors.js",
  "typings": "./dist/cors.d.ts",
  "engines": {
    "node": ">=9"
  },
  "gitHead": "cec73cfa8cbfe5bd8422f1ea53f1711330514cc3"
}
