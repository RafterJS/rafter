{
  "name": "@rafter/di-container",
  "description": "A DI container that works",
  "version": "0.2.2",
  "author": "Josh Stuart <crimsonronin@gmail.com>",
  "keywords": [
    "di",
    "factory",
    "dependency injection",
    "ioc",
    "inversion of control",
    "solid"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/crimsonronin/box-di.git"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "yarn clean && yarn build:lib",
    "build:lib": "babel ./index.js --out-dir ./dist && babel ./lib --out-dir ./dist/lib",
    "clean": "rimraf ./dist",
    "clean:modules": "rimraf ./node_modules",
    "lint": "eslint ./index.js ./babel.config.js \"./lib/**/*\"",
    "format": "prettier --write \"{lib}/**/*.{js,jsx,ts,tsx}\"",
    "test": "jest --notify",
    "update:latest": "yarn upgrade --latest"
  },
  "dependencies": {
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@rafter/babel-preset-rafter": "*",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@rafter/eslint-config": "*"
  }
}
