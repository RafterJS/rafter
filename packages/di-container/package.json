{
  "name": "@rafter/di-container",
  "description": "A DI container that works",
  "version": "0.2.2",
  "author": "Josh Stuart <crimsonronin@gmail.com>",
  "keywords": [
    "di",
    "factory",
    "dependency injection",
    "ioc",
    "inversion of control",
    "solid"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/crimsonronin/box-di.git"
  },
  "main": "./dist/index.js",
  "scripts": {
    "start": "",
    "build:lib": "babel ./index.js --out-dir ./dist && babel ./lib --out-dir ./dist/lib",
    "build": "yarn clean && yarn build:lib",
    "clean": "rimraf ./dist",
    "lint": "eslint ./lib/**",
    "format": "prettier --write \"{lib}/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "lodash.clonedeep": "4.5.0",
    "lodash.find": "4.6.0",
    "lodash.foreach": "4.5.0",
    "lodash.get": "4.4.2",
    "lodash.isfunction": "3.0.9",
    "lodash.isundefined": "3.0.1",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@rafter/babel-preset-rafter": "*",
    "@babel/plugin-proposal-decorators": "7.4.4"
  }
}
