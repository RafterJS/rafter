{
  "name": "@rafterjs/logger-plugin",
  "version": "0.4.1",
  "contributors": [
    {
      "name": "Josh Stuart",
      "email": "joshstuartx@gmail.com"
    }
  ],
  "license": "GNU GPLv3",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshystuart/rafter.git"
  },
  "scripts": {
    "build": "yarn clean:lib && yarn build:lib && yarn build:types",
    "start:watch": "nodemon --watch \"./lib\" --exec \"yarn build\"",
    "build:lib": "babel ./lib --out-dir ./dist --extensions .js,.jsx,.ts,.tsx --source-maps",
    "build:types": "tsc --emitDeclarationOnly",
    "clean:lib": "rimraf ./dist",
    "copy": "copyfiles ./package.json ./dist/package.json",
    "format": "prettier --write \"lib/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"./lib/**/*\" ./babel.config.js",
    "types": "tsc",
    "update:latest": "yarn upgrade --latest"
  },
  "dependencies": {
    "winston": "3.2.1",
    "winston-transport": "4.3.0",
    "logform": "2.1.2"
  },
  "devDependencies": {
    "@rafterjs/babel-preset-rafter": "^0.4.1",
    "@rafterjs/eslint-config": "^0.4.0"
  },
  "description": "Utils for Rafter projects",
  "main": "./dist/logger.js",
  "module": "./dist/logger.js",
  "typings": "./dist/logger.d.ts",
  "engines": {
    "node": ">=9"
  },
  "gitHead": "cec73cfa8cbfe5bd8422f1ea53f1711330514cc3"
}
