{
  "name": "@rafterjs/logger-plugin",
  "version": "0.5.0",
  "contributors": [
    {
      "name": "Josh Stuart",
      "email": "joshstuartx@gmail.com"
    }
  ],
  "license": "GPL-3.0-or-later",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshystuart/rafter.git"
  },
  "scripts": {
    "build": "yarn build:lib && yarn build:types",
    "build:lib": "babel ./lib --out-dir ./dist --extensions .js,.jsx,.ts,.tsx --source-maps",
    "build:types": "tsc --emitDeclarationOnly",
    "copy": "copyfiles ./package.json ./dist/package.json",
    "format": "prettier --write \"lib/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"./lib/**/*\" ./babel.config.js",
    "types": "tsc",
    "update:latest": "yarn upgrade --latest"
  },
  "dependencies": {
    "logform": "2.2.0",
    "winston": "3.3.3",
    "winston-transport": "4.4.0"
  },
  "devDependencies": {
    "@rafterjs/babel-preset-rafter": "^0.5.0",
    "@rafterjs/eslint-config": "^0.5.0"
  },
  "description": "Utils for Rafter projects",
  "main": "./dist/logger.js",
  "module": "./dist/logger.js",
  "typings": "./dist/logger.d.ts",
  "engines": {
    "node": ">=9"
  },
  "gitHead": "a1f017d6435d2fd58ab8bfcaee868f5d63956807"
}
