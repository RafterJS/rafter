{
  "name": "@rafter/di-autoloader",
  "version": "0.2.2",
  "author": "Josh Stuart <joshstuartx@gmail.com>",
  "description": "An autoloading DI container",
  "keywords": [
    "di",
    "factory",
    "dependency injection",
    "ioc",
    "inversion of control",
    "solid"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "yarn clean:lib && yarn build:lib && yarn build:types",
    "build:lib": "babel --include-dotfiles ./lib --out-dir ./dist --extensions .js,.jsx,.ts,.tsx --source-maps",
    "build:types": "tsc --emitDeclarationOnly",
    "clean:lib": "rimraf ./dist",
    "format": "prettier --write \"{lib,test}/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"./lib/**/*\" \"./test/**/*\" ./babel.config.js",
    "test": "jest --notify",
    "types": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshystuart/rafter.git"
  },
  "license": "GNU GPLv3",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@rafter/utils": "^0.2.2",
    "awilix": "^4.2.5",
    "is-class": "^0.0.9",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@rafter/babel-preset-rafter": "^0.2.2",
    "@rafter/eslint-config": "^0.2.2",
    "@types/lodash": "^4.14.149"
  },
  "engines": {
    "node": ">=9"
  },
  "gitHead": "38b9768e511d049cc5a0d113d760af5bac8e13ce"
}
