{
  "name": "@rafter/di-autoloader",
  "version": "0.3.1",
  "author": "Josh Stuart <joshstuartx@gmail.com>",
  "description": "An autoloading DI container",
  "keywords": [
    "di",
    "factory",
    "dependency injection",
    "ioc",
    "inversion of control",
    "solid"
  ],
  "main": "dist/index.js",
  "scripts": {
    "start": "",
    "test": "jest --notify",
    "build:server": "babel ./index.js --out-dir ./dist && babel ./lib --out-dir ./dist/lib",
    "build": "yarn clean && yarn build:server",
    "clean": "rimraf ./dist",
    "lint": "./node_modules/.bin/eslint ./lib ./test",
    "format": "prettier --check \"{lib,test}/**/*.{js,jsx,ts,tsx}\"",
    "format:fix": "prettier --write \"{lib,test}/**/*.{js,jsx,ts,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crimsonronin/box-di-autoloader.git"
  },
  "license": "MIT",
  "dependencies": {
    "@rafter/di-container": "0.2.2"
  },
  "devDependencies": {
    "@rafter/babel-preset-rafter": "*"
  },
  "engines": {
    "node": ">9"
  }
}
