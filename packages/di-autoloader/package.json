{
  "name": "@rafter/di-autoloader",
  "version": "0.3.1",
  "author": "Josh Stuart <joshstuartx@gmail.com>",
  "description": "An autoloading DI container",
  "keywords": [
    "di",
    "factory",
    "dependency injection",
    "ioc",
    "inversion of control",
    "solid"
  ],
  "main": "dist/index.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "yarn clean:lib && yarn build:server && yarn build:types",
    "build:server": "babel --include-dotfiles ./lib --out-dir ./dist --extensions .js,.jsx,.ts,.tsx --source-maps",
    "build:types": "tsc --emitDeclarationOnly",
    "build:index": "babel index.ts --out-dir ./dist --extensions .js,.jsx,.ts,.tsx --source-maps",
    "clean:lib": "rimraf ./dist",
    "format": "prettier --write \"{lib,test}/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"./lib/**/*\" \"./test/**/*\" ./index.ts ./babel.config.js",
    "test": "jest --notify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crimsonronin/box-di-autoloader.git"
  },
  "license": "MIT",
  "dependencies": {
    "awilix": "^4.2.3",
    "@rafter/utils": "*",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@rafter/babel-preset-rafter": "*",
    "@rafter/eslint-config": "*"
  },
  "engines": {
    "node": ">=9"
  }
}
