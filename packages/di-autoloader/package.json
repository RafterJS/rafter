{
  "name": "@rafter/di-autoloader",
  "version": "0.3.1",
  "author": "Josh Stuart <joshstuartx@gmail.com>",
  "description": "An autoloading DI container",
  "keywords": [
    "di",
    "factory",
    "dependency injection",
    "ioc",
    "inversion of control",
    "solid"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn clean && yarn build:server",
    "build:server": "babel ./index.js --out-dir ./dist && babel ./lib --out-dir ./dist/lib --ignore \"**/*.spec.js\"",
    "clean": "rimraf ./dist",
    "clean:modules": "rimraf ./node_modules",
    "format": "prettier --write \"{lib,test}/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"./lib/**/*\" \"./test/**/*\" ./index.js",
    "test": "jest --notify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crimsonronin/box-di-autoloader.git"
  },
  "license": "MIT",
  "dependencies": {
    "@rafter/di-container": "*"
  },
  "devDependencies": {
    "@rafter/babel-preset-rafter": "*"
  },
  "engines": {
    "node": ">=9"
  }
}
